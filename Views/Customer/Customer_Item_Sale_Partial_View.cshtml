@model List<eSalesPro.Models.Sale>

@foreach (var v in Model)
{
    <tr>
        <td class="text-center" data-title="User ID">@v.Created_at.ToShortDateString()</td>
        <td class="text-center" data-title="User ID">@v.Product.Custom_Label_</td>
        <td class="text-center" data-title="User ID">@v.Product.Item_Title</td>
        <td class="text-center" data-title="User ID">@v.Quantity</td>
        <td class="text-center" id="" data-title="User ID">@v.Price</td>
        <td class="text-center" data-title="User ID">@{var diff = v.PriceVAT - v.Price;}@diff</td>
        <td class="text-center" data-title="User ID">@v.PriceVAT</td>
    </tr>
}